<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title Vou de Freela</title>
    <link href="/Content/css/bootstrap.min.css" rel="stylesheet" />
    <link href="/Content/css/font-awesome.min.css" rel="stylesheet" />
    <link href="/Content/css/main.css" rel="stylesheet" />
</head>
<body>
    <main class="container left-container">
        <div class="row">
            <div id="menu-target">
                <ul>
                    <li><i class="fa fa-home"></i><a href="@Url.Action("About", "Postagem")">Home</a></li>
                    <li><i class="fa fa-anchor"></i><a href="@Url.Action("CreateFree", "Profissionals")">Faça Parte (free)</a></li>
					<li><i class="fa fa-anchor"></i><a href="@Url.Action("Create", "Profissionals")">Faça Parte (premium)</a></li>
                    <li><i class="fa fa-paper-plane"></i><a href="@Url.Action("Contato", "Profissionals")">Fale Conosco</a></li>
                </ul>
                <hr>

            </div>
            <section class="sidebar col-md-4 col-sm-12" style="background-image:url(/Content/img/home_default.jpg)">
                <span class="menu-trigger animated fadeInDown">
                    <span class="bar"></span>
                    <span class="bar"></span>
                    <span class="bar"></span>
                </span>

                <!--<div class="site-info">
                    <div class="primary-info">
                        <h1>Vou de Freela</h1>
                        <p>Aqui uma mensagem bonitinha</p>
                        <p>Aqui outra mensagem bonitinha</p>
                    </div>
                    <div class="secondary-info">
                        <p>
                            <a class="btn btn-primary" href="#"><i class="fa fa-user-plus"></i>Faça parte!</a>
                        </p>
                    </div>
                </div>-->
            </section><!-- end sidebar -->

            @RenderBody()
        </div>

    </main>


    @Scripts.Render("~/bundles/jquery")
    @*@Scripts.Render("~/bundles/bootstrap")*@

    @*<script src="../../../../../ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js"></script>*@
    <script>window.jQuery || document.write('<script src="js/vendor/jquery-1.11.2.min.js"><\/script>')</script>
    <script src="~/Content/js/vendor/jquery.jpanelmenu.min.js"></script>
    <script src="~/Content/js/vendor/bootstrap.min.js"></script>
    <script src="~/Content/js/vendor/fastclick.min.js"></script>
    <script src="~/Content/js/main.js"></script>

    <script>
        (function (i, s, o, g, r, a, m) {
            i['GoogleAnalyticsObject'] = r; i[r] = i[r] || function () {
                (i[r].q = i[r].q || []).push(arguments)
            }, i[r].l = 1 * new Date(); a = s.createElement(o),
            m = s.getElementsByTagName(o)[0]; a.async = 1; a.src = g; m.parentNode.insertBefore(a, m)
        })(window, document, 'script', 'https://www.google-analytics.com/analytics.js', 'ga');

        ga('create', 'UA-96040922-1', 'auto');
        ga('send', 'pageview');

    </script>


    @RenderSection("scripts", required: false)
</body>
</html>
